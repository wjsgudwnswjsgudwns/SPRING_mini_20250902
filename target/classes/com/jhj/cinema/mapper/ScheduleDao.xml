<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="com.jhj.cinema.dao.ScheduleDao">
    
    	<select id="getAllSchedules" resultType="com.jhj.cinema.dto.ScheduleDto">
    		SELECT * FROM schedule
    	</select>
    
    	<insert id="reservationScheduleDao">
    		INSERT INTO schedule(scheduleid, movieid) VALUES (#{param1},#{param2})
    	</insert>
    </mapper>